{
  "uid": "zerotwo-ai-assistant",
  "type": "app",
  "config": {
    "description": "AI-powered assistant that integrates with HubSpot CRM to help manage contacts, companies, deals, and tickets through natural language conversations via MCP.",
    "name": "ZeroTwo AI Assistant",
    "logo": "/app/app-logo.png",
    "distribution": "marketplace",
    "isUserLevel": true,
    "auth": {
      "type": "oauth",
      "redirectUrls": [
        "http://localhost",
        "http://localhost:3000/auth/callback",
        "https://zerotwo.ai/auth/callback"
      ],
      "requiredScopes": [
        "oauth",
        "crm.objects.contacts.read",
        "crm.objects.companies.read",
        "crm.objects.deals.read"
      ],
      "optionalScopes": []
    },
    "permittedUrls": {
      "fetch": ["https://api.hubapi.com"],
      "iframe": [],
      "img": []
    },
    "support": {
      "supportEmail": "support@zerotwo.ai",
      "documentationUrl": "https://zerotwo.ai/docs",
      "supportUrl": "https://zerotwo.ai/support",
      "supportPhone": "+18005555555"
    }
  }
}

